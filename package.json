{
  "name": "adlm",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "apollo-boost": "0.1.6",
    "apollo-server-express": "1.3.6",
    "bcrypt": "2.0.1",
    "body-parser": "1.18.2",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "graphql": "0.13.2",
    "graphql-tools": "3.0.1",
    "knex": "0.14.6",
    "luxon": "^1.2.0",
    "pg": "7.4.3",
    "react": "16.3.2",
    "react-apollo": "2.1.4",
    "react-dom": "16.3.2",
    "react-scripts": "1.1.4",
    "uuid": "3.2.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "client": "react-app-rewired start",
    "server": "concurrently \"yarn server:build --watch\" \"yarn server:start\"",
    "server:build": "tsc --project ./tsconfig.server.json",
    "server:start": "nodemon -r ./require.js -r dotenv/config ./build-server/server/index.js --watch src --watch build-server --ext js,graphql",
    "generate": "ts-node -r ./require.js -r dotenv/config -r --project ./tsconfig.server.json ./src/generator/index.ts",
    "db:dump": "ts-node -r dotenv/config -r --project ./tsconfig.server.json ./src/generator/dump_schema.ts",
    "db:seed": "knex seed:run",
    "db:import": "ts-node -r ./require.js -r --project ./tsconfig.server.json ./src/generator/import_db.ts"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always",
    "trailingComma": "es5"
  },
  "nodemonConfig": {},
  "devDependencies": {
    "@types/bcrypt": "2.0.0",
    "@types/express": "4.11.1",
    "@types/graphql": "0.13.1",
    "@types/jest": "22.2.3",
    "@types/knex": "0.14.14",
    "@types/luxon": "^0.5.2",
    "@types/node": "9.4.7",
    "@types/pg": "7.4.8",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.3.14",
    "@types/react-dom": "16.0.5",
    "@types/uuid": "3.4.3",
    "apollo-codegen": "0.19.1",
    "change-case": "3.0.2",
    "concurrently": "3.5.1",
    "graphql-code-generator": "0.9.1",
    "husky": "0.14.3",
    "nodemon": "1.17.4",
    "prettier": "1.12.1",
    "react-app-rewired": "1.5.2",
    "ts-loader": "4.3.0",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.8.3"
  }
}
